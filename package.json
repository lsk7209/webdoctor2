{
  "name": "koreseo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build || true",
    "build:cloudflare": "next build && npx @cloudflare/next-on-pages --experimental-static",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run build:cloudflare && wrangler pages deploy .vercel/output/static",
    "db:create": "wrangler d1 create webdoctor-db",
    "db:migrate": "wrangler d1 execute webdoctor-db --file=./migrations/0001_initial_schema.sql",
    "db:migrate:local": "wrangler d1 execute webdoctor-db --local --file=./migrations/0001_initial_schema.sql",
    "db:check": "wrangler d1 execute webdoctor-db --command=\"SELECT name FROM sqlite_master WHERE type='table' ORDER BY name;\"",
    "db:check:local": "wrangler d1 execute webdoctor-db --local --command=\"SELECT name FROM sqlite_master WHERE type='table' ORDER BY name;\""
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/xml2js": "^0.4.14",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.0",
    "jose": "^5.9.0",
    "next": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "robots-parser": "^3.0.1",
    "sitemap": "^9.0.0",
    "tailwind-merge": "^2.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.12.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.0",
    "wrangler": "^3.0.0"
  }
}
